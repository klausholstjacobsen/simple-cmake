{
  "version": 2,
  "configurePresets": [
    {
      "name": "default",
      "hidden": true,
      "generator": "Unix Makefiles",
      "cacheVariables": {
        "CMAKE_PROJECT_TOP_LEVEL_INCLUDES": "${workspaceFolder}/conan_provider.cmake",
        "CONAN_BUILD_PROFILE": "${workspaceFolder}/profiles/x86_64-pc-linux-gnu.profile"
      }
    },
    {
      "name": "Linux",
      "inherits": "default",
      "hidden": true,
      "cacheVariables": {
        "CONAN_HOST_PROFILE": "${workspaceFolder}/profiles/x86_64-pc-linux-gnu.profile"
      }
    },
    {
      "name": "Linux - Release",
      "binaryDir": "${workspaceFolder}/out/linux/Release",
      "inherits": "Linux",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CONAN_INSTALL_ARGS": "--settings:host=build_type=Release;--settings:host='&:build_type=Release';--output-folder=${sourceDir};-c:h='&:user.conf:binary_dir=${workspaceFolder}/out/linux/Release'"
      } 
    },
    {
      "name": "Linux - Debug",
      "binaryDir": "${workspaceFolder}/out/linux/Debug",
      "inherits": "Linux",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CONAN_INSTALL_ARGS": "--settings:host=build_type=Release;--settings:host='&:build_type=Debug';--output-folder=${sourceDir};-c:h='&:user.conf:binary_dir=${workspaceFolder}/out/linux/Debug'"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "Linux - Release",
      "configurePreset": "Linux - Release",
      "jobs": 16
    },
    {
      "name": "Linux - Debug",
      "configurePreset": "Linux - Debug",
      "jobs": 16
    }
  ],
  "testPresets": [
  ]
}